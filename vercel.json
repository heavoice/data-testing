{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs18.x",
      "includeFiles": ["node_modules/.prisma/**", "prisma/schema.prisma"]
    }
  },
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
